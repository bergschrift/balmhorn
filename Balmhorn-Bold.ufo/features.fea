#####################################################################
## GSUB
#####################################################################

#--------------------------------------------------------------------
# frac - Fractions
#--------------------------------------------------------------------

feature frac {
    sub one slash two by onehalf;
    sub one slash four by onequarter;
    sub three slash four by threequarters;
    sub one slash three by onethird;
    sub two slash three by twothirds;
    sub one slash eight by oneeighth;
    sub three slash eight by threeeighths;
    sub five slash eight by fiveeighths;
    sub seven slash eight by seveneighths;
} frac;


#--------------------------------------------------------------------
# liga - Ligatures
#--------------------------------------------------------------------

feature liga {
    sub f i by fi;
    sub f l by fl;
} liga;


#--------------------------------------------------------------------
# subs - Subscript/Inferior
#--------------------------------------------------------------------

feature subs {
    sub @Numbers by @Subscript;
    sub parenleft by parenleft.inferior;
    sub parenright by parenright.inferior;
} subs;


#--------------------------------------------------------------------
# sups - Superscript/Superior
#--------------------------------------------------------------------

feature sups {
    sub @Numbers by @Superscript;
    sub parenleft by parenleft.superior;
    sub parenright by parenright.superior;
} sups;


#--------------------------------------------------------------------
# locl - Localized forms
#--------------------------------------------------------------------

feature locl {
    script dev2; language HIN;
    sub Paach.dv by Paanch.dv;
    sub Aath.dv by Aanth.dv;

    script dev2; language MAR;
    sub Sha.dv by Sha.MAR.dv;
    sub La.dv by La.MAR.dv;

    script deva; language HIN;
    sub Paach.dv by Paanch.dv;
    sub Aath.dv by Aanth.dv;

    script deva; language MAR;
    sub Sha.dv by Sha.MAR.dv;
    sub La.dv by La.MAR.dv;
} locl;


#--------------------------------------------------------------------
# nukt - Nukta consonants
#--------------------------------------------------------------------

lookup nukt_Deva {
    sub Ka.dv Nukta.dv by KaNukta.dv;
    sub Kha.dv Nukta.dv by KhaNukta.dv;
    sub Ga.dv Nukta.dv by GaNukta.dv;
    sub Ja.dv Nukta.dv by JaNukta.dv;
    sub Dda.dv Nukta.dv by DdaNukta.dv;
    sub Ddha.dv Nukta.dv by DdhaNukta.dv;
    sub Pha.dv Nukta.dv by PhaNukta.dv;
    sub Ya.dv Nukta.dv by YaNukta.dv;
    sub Na.dv Nukta.dv by NaNukta.dv;
    sub Ra.dv Nukta.dv by RaNukta.dv;
    sub Lla.dv Nukta.dv by LlaNukta.dv;
} nukt_Deva;


lookup nukt_Orya {
    sub Dda.or Nukta.or by Rra.or;
    sub Ddha.or Nukta.or by Rha.or;
} nukt_Orya;


feature nukt {
    script deva; language dflt; lookup nukt_Deva;
    script deva; language HIN;  lookup nukt_Deva;
    script deva; language MAR;  lookup nukt_Deva;

    script dev2; language dflt; lookup nukt_Deva;
    script dev2; language HIN;  lookup nukt_Deva;
    script dev2; language MAR;  lookup nukt_Deva;

    script orya; language dflt; lookup nukt_Orya;
    script ory2; language dflt; lookup nukt_Orya;
} nukt;


